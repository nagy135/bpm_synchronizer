{
  "name": "market-service",
  "version": "1.2.2",
  "description": "Service for nfc market",
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettify && yarn lint:fix"
    }
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/braintree": "^2.22.15",
    "@types/bull": "^3.15.5",
    "@types/express": "^4.17.2",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/uuid": "^7.0.2",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chai-uuid": "^1.0.6",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^8.3.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sinon": "^11.1.1",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.6",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.14.3",
    "@onfido/api": "^2.1.0",
    "axios": "^0.21.1",
    "braintree": "^3.7.0",
    "bull": "^3.29.2",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "winston": "^3.2.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "build": "tsc --build tsconfig.prod.json",
    "build-image": "bash build-image.sh",
    "prettify": "pretty-quick --staged",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "export NODE_ENV=test && ts-mocha --timeout 10000 --runInBand --paths ./tsconfig.json tests/**/*.test.ts --file tests/setup.ts"
  }
}
